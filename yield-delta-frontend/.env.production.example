NEXT_PUBLIC_YEI_PYTH_CONTRACT=0x... # Get from YEI Finance
NEXT_PUBLIC_YEI_REDSTONE_CONTRACT=0x... # Get from YEI Finance

# YEI Finance Oracle Wrappers (Mainnet)
