# ============================================
# PRODUCTION ENVIRONMENT VARIABLES
# Set these in your deployment platform (Railway/Vercel/etc)
# ============================================

# Coinbase Advanced Trade API (Required)
COINBASE_API_KEY_NAME=
COINBASE_API_PRIVATE_KEY=

# API URLs (Update with your deployed URLs)
NEXT_PUBLIC_API_URL=https://your-app.vercel.app/api
AI_ENGINE_URL=https://your-ml-api.railway.app

# ML API Configuration
NEXT_PUBLIC_ML_API_URL=https://your-ml-api.railway.app
NEXT_PUBLIC_ML_WS_URL=wss://your-ml-api.railway.app/ws

# ElizaOS WebSocket
NEXT_PUBLIC_ELIZAOS_WS_URL=wss://your-eliza.railway.app

# Admin Dashboard
NEXT_PUBLIC_ADMIN_ENABLED=true
NEXT_PUBLIC_ADMIN_PATH=/admin

# Monitoring Settings (Production)
NEXT_PUBLIC_METRICS_REFRESH_INTERVAL=60000  # 1 minute
NEXT_PUBLIC_ALERTS_POLL_INTERVAL=120000      # 2 minutes
NEXT_PUBLIC_ENABLE_WEBSOCKET=true

# Feature Flags (Production)
NEXT_PUBLIC_SHOW_ML_METRICS=true
NEXT_PUBLIC_ENABLE_MODEL_TRAINING=false  # Disable in production UI
NEXT_PUBLIC_ENABLE_DRIFT_ALERTS=true

# External Services
NEXT_PUBLIC_SENTRY_DSN=
NEXT_PUBLIC_ANALYTICS_ID=
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Chain Configuration
NEXT_PUBLIC_CHAIN_ID=sei-mainnet
NEXT_PUBLIC_RPC_URL=https://evm-rpc.sei-apis.com

# Contract Addresses (SEI Mainnet - UPDATE THESE)
NEXT_PUBLIC_NATIVE_SEI_VAULT=0x... # Deploy to mainnet
NEXT_PUBLIC_AI_ORACLE=0x... # Deploy to mainnet
NEXT_PUBLIC_VAULT_FACTORY=0x... # Deploy to mainnet

# Active Vault Addresses (Mainnet)
NEXT_PUBLIC_SEI_VAULT_ADDRESS=0x... # Deploy to mainnet
NEXT_PUBLIC_USDC_VAULT_ADDRESS=0x... # Deploy to mainnet

# YEI Finance Oracle Addresses (Mainnet)
