{
  "filter_paths": [
    "contracts/lib",
    "contracts/test",
    "contracts/script"
  ],
  "exclude_paths": [
    "contracts/lib/forge-std",
    "contracts/lib/openzeppelin-contracts"
  ],
  "detectors_to_run": [
    "pragma",
    "solc-version",
    "uninitialized-state",
    "uninitialized-storage",
    "arbitrary-send",
    "controlled-delegatecall",
    "reentrancy-eth",
    "reentrancy-no-eth",
    "tx-origin",
    "unchecked-lowlevel",
    "unchecked-send",
    "suicidal",
    "assembly",
    "assert-state-change",
    "boolean-cst",
    "divide-before-multiply",
    "domain-separator-collision",
    "enum-conversion",
    "erc20-interface",
    "erc721-interface",
    "incorrect-equality",
    "locked-ether",
    "mapping-deletion",
    "shadowing-abstract",
    "shadowing-builtin",
    "shadowing-local",
    "shadowing-state",
    "timestamp",
    "write-after-write"
  ],
  "compilation_warnings": true,
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "json": "slither-report.json",
  "exclude_dependencies": true
}
